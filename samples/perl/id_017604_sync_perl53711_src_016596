sub BODY_16_63() {
	 &mov	("esi",&DWP(4*(9+15+16-14),"esp"));
	&ror	("ecx",18-7);
	 &mov	("edi","esi");
	&ror	("esi",19-17);
	 &xor	("ecx",$T);
	 &shr	($T,3);
	&ror	("ecx",7);
	 &xor	("esi","edi");
	&ror	("esi",17);
	&shr	("edi",10);
	&BODY_00_15(1);
}
sub BODY_00_15() {
    my $in_16_63=shift;
	&mov	("ecx",$E);
	 &mov	("esi",$Foff);
	&ror	("ecx",25-11);
	 &mov	("edi",$Goff);
	&xor	("ecx",$E);
	 &xor	("esi","edi");
	 &mov	($T,&DWP(4*(9+15),"esp"))	if (!$in_16_63);
	&ror	("ecx",11-6);
	 &and	("esi",$E);
	&xor	($E,"ecx");
	 &mov	("ecx",$A);
	&ror	("ecx",22-13);
	 &mov	("edi",$Boff);
	&xor	("ecx",$A);
	 &lea	("esp",&DWP(-4,"esp"));
	&ror	("ecx",13-2);
	 &mov	("esi",&DWP(0,$K256));
	&xor	("ecx",$A);
	&add	($K256,4);
}
